{
    "name": "mytv",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "gh-pages": "^2.0.1",
        "history": "^4.9.0",
        "lodash": "^4.17.11",
        "node-sass": "^4.12.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-intl": "^2.8.0",
        "react-redux": "^7.0.3",
        "react-router-dom": "^5.0.0",
        "react-scripts": "3.0.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "seamless-immutable": "^7.1.4"
    },
    "scripts": {
        "lint": "eslint src -c .eslintrc --ext js,jsx",
        "lint-fix": "eslint --fix .",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "cross-env CI=true react-scripts test --env=jsdom --setupTestFrameworkScriptFile=./setupTests",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.12.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.2.0",
        "eslint-import-resolver-webpack": "^0.11.1",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^2.2.0",
        "lint-staged": "^8.1.5",
        "prettier": "^1.17.0",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm test",
            "pre-push": "npm run lint && npm test"
        }
    },
    "homepage": "https://deepakkadarivel.github.io/mytv"
}
